\n  }\n  @for $i from 1 through 32 {\n    &:nth-child(#{$i}) .image {\n      @include image($idx: $i, $type: \"png\");\n    }\n  }\n\n  $list: orange, royalblue, yellow;\n\n  $map: (\n    o: orange,\n    r: royalblue,\n    y: yellow,\n  );\n  @each $c in $list {\n    .box {\n      color: $c;\n    }\n  }\n  @each $k, $v in $map {\n    .box {\n      color: $v;\n    }\n  }\n}\n","logo.scss":".logo {\n  max-width: 300px;\n  margin: 0 auto;\n  padding: 0 20px;\n  img {\n    max-width: 100%;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"73fcbf4173e857c9babe75ab03e2d66a","cacheData":{"env":{}}}